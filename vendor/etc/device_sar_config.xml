        <!-- wifi,hotspot(1),modem,sarsensor(1),hall,audio(1),mcc,btpan(1),bt(1),sarsensor2(1),cable(1),usb,use,radiopower,sim,sarsensor3(1) -->
        <listeners>0101010111000001</listeners>
        <!-- hotspot,sarSensor audio -->
        <dsimap value="0F1">0</dsimap>
        <dsimap value="010">1</dsimap>
        <dsimap value="000">3</dsimap>
        <dsimap value="1FF">4</dsimap>
